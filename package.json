{
  "name": "glorified-todo-list",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "client": "vite serve client",
    "server": "nodemon -r dotenv/config server/index.ts",
    "db:migrate": "mikro-orm migration:up",
    "db:migrate:undo": "mikro-orm migration:down",
    "db:migrate:undo:all": "mikro-orm migration:down --to 0",
    "db:migration:create": "npx mikro-orm migration:create --blank",
    "db:seed:all": "npx mikro-orm seeder:run",
    "db:seed:users": "npx mikro-orm seeder:run --class=UserSeeder",
    "db:seed:tasks": "npx mikro-orm seeder:run --class=TaskSeeder",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mikro-orm/core": "^5.6.7",
    "@mikro-orm/migrations": "^5.6.7",
    "@mikro-orm/postgresql": "^5.6.7",
    "@mikro-orm/seeder": "^5.6.7",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "axios": "^1.2.2",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.21.0",
    "readline-sync": "^1.4.10",
    "utility-types": "^3.10.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/eslint-plugin": "^7.19.1",
    "@extensionengine/eslint-config": "^3.0.1",
    "@mikro-orm/cli": "^5.6.7",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/readline-sync": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "eslint": "^8.32.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-sort": "^1.3.0",
    "eslint-plugin-vue": "^9.8.0",
    "nodemon": "^2.0.20",
    "sass": "^1.57.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  },
  "engines": {
    "node": ">=19.0.0",
    "npm": ">=9.3.0",
    "postgres": ">=13.9"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "server/config/mikro-orm.config.ts",
      "server/dist/mikro-orm.config.js"
    ]
  }
}
